{
  "name": "abstract-snake-tree",
  "main": "src/index.ts",
  "version": "0.0.1",
  "private": true,
  "description": "Node library to parse and write python scripts",
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "esbuild": "^0.17.8",
    "eslint": "^8.26.0",
    "tree-sitter-cli": "^0.20.8",
    "tsconfig": "*",
    "tsx": "^3.12.1",
    "typescript": "^5.0.4",
    "vitest": "^0.30.0"
  },
  "scripts": {
    "type-check": "tsc -p ./ --noEmit",
    "test": "vitest run --no-threads",
    "test-watch": "vitest --no-threads",
    "build-python-grammar": "tree-sitter build-wasm ../../node_modules/tree-sitter-python; mv tree-sitter-python.wasm assets/tree-sitter-python.wasm"
  },
  "dependencies": {
    "tree-sitter-python": "^0.20.1",
    "ui-node-definitions": "*",
    "web-tree-sitter": "^0.20.8"
